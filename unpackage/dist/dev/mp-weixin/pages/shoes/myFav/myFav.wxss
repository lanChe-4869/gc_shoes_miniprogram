
.schBox{
	width: 592rpx;
	height:70rpx;
	padding-left: 30rpx;
	padding-right: 10rpx;
	border-radius: 100rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	text-indent: 4rpx;
	background-color: #E1E1E1;
}
.sch-btn {
	width: 102rpx;
	height: 54rpx;
	background: #FFFFFF;
	opacity: 0.9;
	border-radius: 40rpx;
	font-size: 26rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}
.favPane{
	width: 630rpx;
	height:100%;
	padding: 22rpx 32rpx 22rpx 50rpx;
	height:auto;
	border-radius: 16rpx;
	background-color: #FFFFFF;
	display: flex;
	flex-direction: column; 
	justify-content: center;
	align-items: center;
	margin-top: 40rpx;
}
.sortList{
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin-bottom: 52rpx;
}
.stLi{
	display: flex;
	align-items: center;
	margin-right: 40rpx;
}
.stLi>image{
	width: 26rpx;
	margin-left: 10rpx;
}
.shoesList{
	display: flex;
	flex-direction: column;
	align-items: center;
}
.shoesLi{
	width: 636rpx;
	height: 192rpx;
	display: flex; 
	justify-content: space-between;
	margin-bottom: 40rpx;
}
.shoesLi-img{
	width:202rpx;
	height:192rpx;
}
.shoesLi-text{
	width: 406rpx;
	height:100%;
	display: flex;
	flex-direction: column;
	justify-content: center;
}
.shoesLi-text-title{
	font-size: 36rpx;
	color:#474747;
	margin-bottom: 12rpx;
}
.shoesLi-text-content{
	font-size: 26rpx;
	color:#4F4F4F;
	margin-bottom: 34rpx;
}
.shoesLi-price{
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.shoesLi-price-text{
	font-size: 40rpx;
	color:#4797EB;
}
.shoesLi-price-btn{
	width: 110rpx;
	height:48rpx;
	border-radius: 40rpx;
	border:1px solid #CACACA;
	font-size: 26rpx;
	color:#000000;
	background-color: #ffffff;
	display: flex;
	justify-content: center;
	align-items: center;
	margin:0px;
}
.shoesLi-price-btn:after{
	border: 0px;
}
.favEdit{
	width: 44rpx;
}


