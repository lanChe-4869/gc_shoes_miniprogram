
.liPane{
	width: 100%;
	height: 100%;
	padding: 40rpx 40rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
}
.shoesLi{
	width: 636rpx;
	height: 192rpx;
	display: flex; 
	justify-content: space-between;
	margin-bottom: 40rpx;
}
.shoesLi-img{
	width:202rpx;
	height:192rpx;
}
.shoesLi-text{
	width: 406rpx;
	height:100%;
	display: flex;
	flex-direction: column;
	justify-content:space-between;
}
.shoesLi-text-title{
	font-size: 36rpx;
	height:100rpx;
	white-space: pre-wrap;
	overflow: hidden;
	color:#474747;
	margin-bottom: 12rpx;
}
.shoesLi-text-content{
	font-size: 26rpx;
	color:#4F4F4F;
	margin-bottom: 34rpx;
}
.shoesLi-price{
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.shoesLi-price-text{
	font-size: 40rpx;
	color:#4797EB;
}
.shoesLi-price-btn{
	padding:0 30rpx;
	height:48rpx;
	border-radius: 20rpx;
	border:1px solid #CACACA;
	font-size: 26rpx;
	color:#000000;
	background-color: #ffffff;
	display: flex;
	justify-content: center;
	align-items: center;
	margin:0px;
}
.shoesLi-price-btn:after{
	border: 0px;
}
.copyPane{
	width:500rpx;
	height:500rpx;
	margin:auto auto;
	background-color: #FFFFFF;
}

