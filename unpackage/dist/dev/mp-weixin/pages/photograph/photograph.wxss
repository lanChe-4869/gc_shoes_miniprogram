
page {
	background-color: #F6F6F6;
}
.g-header {
	margin-top: 96rpx;
	width: 698rpx;
	padding: 0 26rpx;
	height: 64rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.header-sch {
	padding-top: 20rpx;
}
.g-main {
	width: 750rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.takePhoto {
	position: absolute;
	right: 20rpx;
	top: 640rpx;
	border-radius: 50%;
	padding: 10rpx 10rpx;
	background-color: #F6F6F6;
}
.resTable {
	width: 582rpx;
}
.res-tr {
	height: 50rpx;
	width: 100%;
	padding: 10rpx 0;
	display: flex;
	align-items: center;
	font-size: 30rpx;
	color: #4B4B4B;
}
.res-title {
	width: 260rpx;
}
.res-left {
	width: 80rpx;
}
.res-unit {
	width: 30rpx;
	margin-left: 14rpx;
}
.res-right {
	margin-left: 50rpx;
	width: 80rpx;
}

